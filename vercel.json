{
  "redirects": [
    {
      "source": "http://ldevents.co.ke/:path*",
      "destination": "https://ldevents.co.ke/:path*",
      "permanent": true
    },
    {
      "source": "https://www.ldevents.co.ke/:path*",
      "destination": "https://ldevents.co.ke/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(service-worker.js|sitemap.xml|robots.txt|manifest.json)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}

